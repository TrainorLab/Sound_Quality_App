# (add this at the beginning)
[app]

title = Sound_Quality_App

# (add this line)
package.name = sound.quality.app

# (add this line)
source.dir = .

# (add this line)
source.include_exts = py,png,jpg,kv,atlas

# (add this line)
version = 0.1

# (add this line)
requirements = python3, kivy, datetime, pytz

# (uncomment and set this to False)
orientation = portrait

# (add this line)
fullscreen = 1

# (add this line)
android.permissions = INTERNET,WRITE_EXTERNAL_STORAGE

# (add this line)
android.arch = arm64-v8a

# (add this line)
android.api = 31

# (add this line)
android.minapi = 21

